package DemoS;

import java.util.Scanner;

public class PrimeNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a;
		String extra= "";
	
		Scanner scanner=new Scanner(System.in);
		System.out.println("Enter the Number");
		a=scanner.nextInt();
		
		for(int i = 2;i<=a;i++)
		{
			if(a%i==0)
			{
				extra = extra +i+"";
				
			}
			
		}
		
//		if(extra.equals(a))
//		{
//			
//			
//			
//		}
//		
	}

}
